/**app.wxss**/
@import "wxParse/wxParse.wxss";

image {
  width: 100%;
  display: block;
}
.logo {
  width: 100%;
  height: 105.5rpx;
  max-height: 105.5rpx;
  overflow: hidden;
}
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 
.foot {
  display: flex;
  width: 100%;
  justify-content: space-between;
  text-align: center;
  background: #eeeeee;
  padding:20rpx 0 ; 
  position: fixed;
  bottom: 0;
  z-index: 10000
}
.yytitle {
  font-size: 35rpx;
}
.footnav {
  width: 20%;
  position: relative;
  font-size: 22rpx;
  display: inline-block;
 padding-top: 30px;
}
.footnav image {
  width: 53rpx;
  height: 53rpx;
  position:absolute;
  left: 50%;
  top:0;
  transform: translateX(-50%);
}
.footer {
  margin-top: 20rpx;
  background: #fff;
  padding: 50rpx 3%;
  padding-bottom: 200rpx;

}
.footer view {
  text-align: left;
    font-size: 28rpx;
    color: #888;
    margin-left: 80rpx;
    line-height: 50rpx;
}